package Programmers.Lv1;

public class S230110_나머지가1이되는수찾기 {

	public static void main(String[] args) {

		int n = 10;
		int answer = 0;
        for(int i = 1; i <= n; i++){
            if(n % i == 1){
                answer = i;
                break;
            }
        }
        System.out.println(answer);
	}

}
