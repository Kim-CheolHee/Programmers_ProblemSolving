package Programmers.Lv1;

public class S230111_두정수사이의합 {

	public static void main(String[] args) {
		
		int a = 3;
		int b = 5; 
		long answer = 0;
        if(a > b){
            for(int i = b; i <= a; i++){
                answer += i;
            }
        } else if(a < b){
            for(int i = a; i <= b; i++){
                answer += i;
            }
        } else if(a == b){
            answer = a;
        }
		System.out.println(answer);
	}

}
