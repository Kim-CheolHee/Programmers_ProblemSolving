package Programmers.Lv1;

public class S230114_가운데글자가져오기 {

	public static void main(String[] args) {

		String s = "abcde";
//		String s = "qwer";
		String answer = "";

		if (s.length() % 2 == 0) {
			answer += String.valueOf(s.charAt(s.length() / 2 - 1));
			answer += String.valueOf(s.charAt(s.length() / 2));
		} else {
			answer += String.valueOf(s.charAt(s.length() / 2));
		}

		System.out.println(answer);
	}

}
