package Programmers.Lv1;

public class S230107_x만큼간격이있는n개의숫자 {

	public static void main(String[] args) {
		int x = 2;
		int n = 5;
		long[] answer = new long[n];

		for (int i = 0; i < n; i++) {
			long y = (long) x * (i + 1);
			answer[i] = y;
		}

//		========================================
//		answer[0] = x;
//
//        for (int i = 1; i < n; i++) {
//            answer[i] = answer[i - 1] + x;
//        }
	}

}
