package Programmers;

import java.util.Arrays;

public class S221021_짝수홀수개수 {

	public static void main(String[] args) {
		int[] num_list = { 1, 2, 3, 4, 5 };
		int[] answer = new int[2];
		int even = 0;
		int odd = 0;
		for (int i = 0; i < num_list.length; i++) {
			if (num_list[i] % 2 == 0) {
				even++;
			}
			if (num_list[i] % 2 == 1) {
				odd++;
			}
		}
		answer[0] = even;
		answer[1] = odd;
		System.out.println(Arrays.toString(answer));
	}

}
