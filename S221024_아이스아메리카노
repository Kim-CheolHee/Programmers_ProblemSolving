package Programmers;

import java.util.Arrays;

public class S221024_아이스아메리카노 {

	public static void main(String[] args) {
		int money = 15000;
		int[] answer = new int[2];
		int ame = money / 5500;
		int change = money % 5500;
		answer[0] = ame;
		answer[1] = change;
		System.out.println(Arrays.toString(answer));

	}

}
