package HomeWork;

import java.util.Scanner;

// 2022. 9. 30. 19:10 김철희

public class Quiz1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		System.out.print("사과의 수 : ");
		int applenum = sc.nextInt();
		int bucketSize = 10;
		int bucketnum = (applenum / bucketSize) + (applenum%10>0?1:0);
		
		if (applenum > 10 && applenum > 0) {
			System.out.println("필요한 바구니의 수 : " + bucketnum);
		} else {
			System.out.println("필요한 바구니의 수 : " + 1);
		}

	}

}
